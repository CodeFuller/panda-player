<cfwpf:WpfApplication x:TypeArguments="vm:ApplicationViewModel"
             x:ClassModifier="internal"
             x:Class="PandaPlayer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:cfwpf="clr-namespace:CodeFuller.Library.Wpf;assembly=CodeFuller.Library.Wpf"
             xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:vm="clr-namespace:PandaPlayer.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <md:BundledTheme BaseTheme="Dark" PrimaryColor="Grey" SecondaryColor="Lime" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="Views/MaterialDesignThemes.Overrides.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/PandaPlayer.DiscAdder;component/Views/MaterialDesignThemes.Overrides.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</cfwpf:WpfApplication>
